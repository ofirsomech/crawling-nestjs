{
  "name": "crawling",
  "version": "0.0.0",
  "description": "",
  "author": "Ofir Somech",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli",
    "hmr": "node dist/server",
    "inspect:lint": "eslint './src/**/*.ts'",
    "inspect:lint:fix": "eslint './src/**/*.ts' --fix",
    "inspect:test": "jest --passWithNoTests",
    "inspect:test:watch": "jest --watch",
    "inspect:test:cov": "jest --coverage --passWithNoTests",
    "inspect:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "inspect:test:e2e": "jest --config ./test/jest-e2e.json",
    "inspect:license": "license-checker --failOn GPLv2 --summary",
    "inspect:vulnerabilities": "npm audit",
    "inspect:outdated": "npm outdated || true",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.ts\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.ts\"",
    "inspect:all": "concurrently -c \"bgBlue.bold,bgMagenta.bold,yellow\" \"npm:inspect:test:cov\" \"npm:inspect:outdated\" \"npm:inspect:lint\" \"npm:inspect:vulnerabilities\" \"npm:inspect:license\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.3.1",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.1",
    "@nestjs/platform-express": "^9.3.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@opentelemetry/auto-instrumentations-node": "^0.36.0",
    "@opentelemetry/core": "^1.9.1",
    "@opentelemetry/exporter-jaeger": "1.8.0",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/sdk-node": "^0.35.1",
    "@opentelemetry/sdk-trace-node": "^1.9.1",
    "@opentelemetry/semantic-conventions": "^1.9.1",
    "@types/jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csurf": "^1.11.0",
    "express-basic-auth": "^1.2.1",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mysql": "^2.18.1",
    "puppeteer": "^19.9.1",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.5",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@nestjs/testing": "9.2.1",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.15",
    "@types/hapi__joi": "17.1.9",
    "@types/helmet": "4.0.0",
    "@types/jest": "29.2.5",
    "@types/joi": "17.2.2",
    "@types/node": "18.11.18",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "concurrently": "7.6.0",
    "eslint": "8.31.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-typescript": "0.14.0",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "license-checker": "25.0.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.2",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
